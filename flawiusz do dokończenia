n = 10
k = 3
a = 0
lista = [i for i in range(1,n+1)]
stos =[]
while len(lista) > 1:
	for i in range(1,len(lista)//k + 1):
		lista.pop(i*k - i)
		#print(lista)
		a = i*k - k
	#print(a)
	del stos[::]
	for x in range(a):
		stos.append(lista[x])
	#print(stos)
	del lista[:len(stos)-1]
	lista.extend(stos[:-1])
	print(lista)
